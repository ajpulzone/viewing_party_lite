<% if session[:user_id] %>

<%= link_to "Log Out", "/logout" %>

<ul id="existing_users">
  <% @users.each do |user| %>
  <li> Name: <%= "#{user.name}"%>, Email: <%= "#{user.email}" %> </li>
  <% end %>
</ul>
<% else %>

<div id='login'>
  <%= link_to 'Login', "/login" %>
</div>
<div id='new_user'>
  <%= button_to 'Create a New User', "/register" %>
</div>
<% end %>
