<h4><%= link_to 'Home', root_path %><h4>
<% if current_user %>
  <h4><%= "Welcome #{current_user.name}" %><h4>
  <h4><%= button_to 'Log Out', logout_path, method: :delete %><h4>
<% else %>
  <h4><%= link_to 'Log In', "/login" %><h4>
  <h4><%= link_to 'Create A User', "/register" %><h4>
<% end %>
<h1> Viewing Party </h1>